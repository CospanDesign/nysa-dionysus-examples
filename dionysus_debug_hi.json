{
	"board":"dionysus",
	"PROJECT_NAME":"example_project",
	"SLAVES":{
		"gpio1":{
			"filename":"wb_gpio.v",
			"bind":{
				"gpio_out[1:0]":{
					"loc":"led[1:0]",
					"direction":"output"
				},
				"gpio_in[3:2]":{
					"loc":"button[1:0]",
					"direction":"input"
				}
			}
  	},
    "lax1":{
      "filename":"wb_logic_analyzer.v",
      "bind":{
        "o_phy_tx":{
          "loc":"s0",
          "direction":"output"
        },
        "i_phy_rx":{
          "loc":"s1",
          "direction":"input"
        }
      }
    }
  },
  "internal_bind":{
    "lax1_i_la_clk" : {
      "signal":"egress_clk"
    },
    "lax1_i_la_data[7:0]":{
      "signal":"d[7:0]"
    },
    "lax1_i_la_data[8]":{
      "signal":"txe_n"
    },
    "lax1_i_la_data[9]":{
      "signal":"wr_n"
    },
    "lax1_i_la_data[10]":{
      "signal":"rxe_n"
    },
    "lax1_i_la_data[11]":{
      "signal":"rd_n"
    },
    "lax1_i_la_data[12]":{
      "signal":"oe_n"
    },
    "lax1_i_la_data[13]":{
      "signal":"siwua"
    },
    "lax1_i_la_data[14]":{
      "signal":"1'b0"
    },
    "lax1_i_la_data[15]":{
      "signal":"button[0]"
    },
    "lax1_i_la_data[31:16]":{
      "signal":"ftdi_debug[15:0]"
    },
    "lax1_i_la_ext_trig":{
      "signal":"button[1]"
    }
  },
  "dependencies":[
    "logic_analyzer.v",
    "dpb.v"
  ]
}
