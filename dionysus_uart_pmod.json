{
    "board":"dionysus",
    "PROJECT_NAME":"example_project",
    "SLAVES":{
        "gpio1":{
            "filename":"wb_gpio.v",
            "bind":{
                "gpio_out[1:0]":{
                    "loc":"led[1:0]",
                    "direction":"output"
                },
                "gpio_in[3:2]":{
                    "loc":"button[1:0]",
                    "direction":"input"
                }
            }
        },
        "uart1":{
            "filename":"wb_uart.v",
            "bind":{
                "o_tx":{
                    "loc":"PMODA2",
                    "direction":"output"
                },
                "i_rx":{
                    "loc":"PMODA3",
                    "direction":"input"
                },
                "o_cts":{
                    "loc":"PMODA1",
                    "direction":"output"
                },
                "i_rts":{
                    "loc":"PMODA4",
                    "direction":"input"
                }
            }
        }
    }
}

